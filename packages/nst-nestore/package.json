{
  "name": "@nst/nestore",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts src/types.ts",
    "dev": "tsup src/index.ts --format esm --watch --dts src/types.ts",
    "//_dev": "tsup src/index.ts --format esm --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@nst/tsconfig": "workspace:*",
    "eslint": "^7.32.0",
    "eslint-config-nst": "workspace:*",
    "react": "^17.0.2",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup":{
    "name": "nestore",
    "entry": ["src/index.ts"],
    "format": ["esm", "cjs"],
    "treeshake": true,
    "sourcemap": false,
    "clean": true,
    "shims": false,
    "platform": "neutral",
    "target": "es2018",
    "bundle": true,
    "replaceNodeEnv": false,
    "minify": false,
    "minifyWhitespace": false,
    "minifyIdentifiers": false,
    "minifySyntax":false
  }
}
