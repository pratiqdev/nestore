{
  "name": "@pratiq/nestore-react",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external @nestate/core",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external @nestate/core",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@nestate/config": "workspace:*",
    "@nestate/core": "workspace:*",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "eslint": "^7.32.0",
    "react": "^17.0.2",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
